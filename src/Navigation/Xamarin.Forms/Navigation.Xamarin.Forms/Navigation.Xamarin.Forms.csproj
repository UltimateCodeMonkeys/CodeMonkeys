    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>CodeMonkeys.Navigation.Xamarin.Forms</AssemblyName>
    <RootNamespace>CodeMonkeys.Navigation.Xamarin.Forms</RootNamespace>
